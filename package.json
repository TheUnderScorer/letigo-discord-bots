{
  "name": "wojciech-discord-bot",
  "version": "2.5.0",
  "description": "Wojciech for your discord server!",
  "main": "index.js",
  "private": true,
  "scripts": {
    "cm": "cz",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "serve": "ts-node-dev src/server/server.ts -p tsconfig.json",
    "build": "tsc -p tsconfig.server.json",
    "deploy:production": "cd deploy && pulumi up -y -s production",
    "deploy:development": "cd deploy && pulumi up -y -s development"
  },
  "engines": {
    "node": "16.x"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.4",
    "@semantic-release/npm": "9.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "27.4.1",
    "@types/node": "18.14.1",
    "@types/node-cron": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "7.0.4",
    "jest": "28.0.3",
    "lint-staged": "12.3.7",
    "prettier": "2.8.4",
    "semantic-release": "19.0.2",
    "ts-jest": "28.0.0-next.3",
    "ts-loader": "^9.2.8",
    "ts-node": "10.7.0",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@discordjs/builders": "1.4.0",
    "@discordjs/rest": "1.5.0",
    "@discordjs/voice": "0.14.0",
    "axios": "1.3.4",
    "discord-api-types": "0.37.35",
    "discord.js": "14.7.1",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "node-cron": "^3.0.2",
    "openai": "^3.1.0",
    "playwright": "^1.25.1",
    "tiny-typed-emitter": "^2.1.0",
    "tweetnacl": "^1.0.3",
    "youtube-search": "^1.1.6",
    "ytdl-core": "^4.11.2"
  }
}
